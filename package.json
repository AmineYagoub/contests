    "suneditor": "^2.44.3",
    "suneditor-react": "^3.4.1",
    "tslib": "^2.4.0",
    "valtio": "^1.7.5"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.0",
    "@graphql-codegen/cli": "2.11.5",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
