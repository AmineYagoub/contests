{
  "name": "contests",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "api": "nx run gateway:serve",
    "cts": "nx run cts:serve",
    "auth": "nx run auth:serve",
    "ui": "nx run frontend:serve --port 8080",
    "start": "nx serve",
    "build": "nx build",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@nrwl/next": "14.5.4",
    "antd": "^4.22.3",
    "core-js": "^3.6.5",
    "echarts": "^5.3.3",
    "echarts-for-react": "^3.0.2",
    "next": "12.2.3",
    "next-transpile-modules": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "stylis-plugin-rtl": "^2.1.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.0",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.5.4",
    "@nrwl/web": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "commitizen": "^4.2.5",
    "cypress": "^10.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "lint-staged": "^13.0.3",
    "nx": "14.5.4",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "npm run lint",
    "*.{js,ts,jsx,tsx,json,md,css,scss,yml}": "prettier --write **/*.{js,ts,jsx,tsx,json,md,css,scss,yml}"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
