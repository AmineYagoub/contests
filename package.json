{
  "name": "contests",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "cts:dev": "(cd apps/contest/ && npx prisma migrate dev)",
    "cts:reset": "(cd apps/contest/ && npx prisma migrate reset && npx prisma generate)",
    "cts:push": "(cd apps/contest/ && npx prisma db push --accept-data-loss)",
    "cts:show": "(cd apps/contest/ && npx prisma studio)",
    "auth:show": "(cd apps/auth/ && npx prisma studio)",
    "auth:dev": "(cd apps/auth/ && npx prisma migrate dev)",
    "auth:push": "(cd apps/auth/ && npx prisma db push --accept-data-loss)",
    "auth:reset": "(cd apps/auth/ && npx prisma migrate reset && npx prisma generate)",
    "codegen": "graphql-codegen --config codegen.yml",
    "api": "nx run gateway:serve",
    "cts": "nx run contest:serve",
    "wss": "nx run wss:serve",
    "auth": "nx run auth:serve",
    "ui": "rimraf apps/frontend/.next && nx run frontend:serve --port 3000",
    "start": "nx serve",
    "build": "nx build",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "prebuild": "rimraf dist",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.6.9",
    "@apollo/gateway": "^2.0.5",
    "@apollo/subgraph": "^2.0.5",
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.1.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/platform-socket.io": "^9.0.11",
    "@nestjs/websockets": "^9.0.11",
    "@nrwl/next": "15.2.0",
    "@prisma/client": "^4.2.0",
    "antd": "^4.22.4",
    "antd-img-crop": "^4.2.5",
    "apollo-server-express": "^3.10.2",
    "argon2": "^0.29.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.24.1",
    "deepmerge": "^4.2.2",
    "echarts": "^5.3.3",
    "echarts-for-react": "^3.0.2",
    "formidable": "^2.0.1",
    "framer-motion": "^7.2.0",
    "googleapis": "^107.0.0",
    "graphql": "^16.6.0",
    "handlebars": "^4.7.7",
    "jszip": "^3.10.1",
    "minio": "^7.0.32",
    "moment-timezone": "^0.5.34",
    "multiparty": "^4.2.3",
    "next": "13.0.0",
    "next-transpile-modules": "^10.0.0",
    "nodemailer": "^6.7.8",
    "passport-jwt": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.9",
    "rxjs": "^7.5.6",
    "socket.io-client": "^4.5.1",
