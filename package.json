    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.0.3",
    "nx": "^15.0.5",
    "prettier": "^2.7.1",
    "prisma": "^4.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.7",
    "ts-node": "~10.9.1",
    "typescript": "~4.7.4",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,css,scss,yml}": "prettier --write **/*.{js,ts,jsx,tsx,json,md,css,scss,yml}"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
