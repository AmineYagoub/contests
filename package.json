{
  "name": "contests",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "contest:dev": "(cd apps/contest/ && npx prisma migrate dev)",
    "contest:reset": "(cd apps/contest/ && npx prisma migrate reset)",
    "codegen": "graphql-codegen --config codegen.yml",
    "api": "nx run gateway:serve",
    "cts": "nx run contest:serve",
    "auth": "nx run auth:serve",
    "ui": "rimraf apps/frontend/.next && nx run frontend:serve --port 8080",
    "start": "nx serve",
    "build": "nx build",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "prebuild": "rimraf dist",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/gateway": "^2.0.5",
    "@apollo/subgraph": "^2.0.5",
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/mercurius": "^10.0.21",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/platform-fastify": "^9.0.8",
    "@nrwl/next": "14.5.4",
    "@prisma/client": "^4.2.0",
    "antd": "^4.22.4",
    "apollo-server-express": "^3.10.0",
    "apollo-server-fastify": "^3.10.0",
    "class-validator": "^0.13.2",
    "core-js": "^3.24.1",
    "deepmerge": "^4.2.2",
    "echarts": "^5.3.3",
    "echarts-for-react": "^3.0.2",
    "formidable": "^2.0.1",
    "framer-motion": "^7.2.0",
    "graphql": "^16.5.0",
    "jszip": "^3.10.1",
    "mercurius": "^10.1.0",
    "moment-timezone": "^0.5.34",
    "next": "12.2.4",
    "next-transpile-modules": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.9",
    "rxjs": "^7.5.6",
    "stylis-plugin-rtl": "^2.1.1",
    "tslib": "^2.4.0",
    "valtio": "^1.6.4"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.0",
    "@faker-js/faker": "^7.4.0",
    "@graphql-codegen/cli": "2.11.5",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nest": "^14.5.4",
    "@nrwl/node": "14.5.4",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.5.4",
    "@nrwl/web": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@testing-library/react": "13.3.0",
    "@types/formidable": "^2.0.5",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-jest": "28.1.3",
    "commitizen": "^4.2.5",
    "cypress": "^10.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "28.1.3",
    "lint-staged": "^13.0.3",
    "nx": "14.5.4",
    "prettier": "^2.7.1",
    "prisma": "^4.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.7",
    "ts-node": "~10.9.1",
    "typescript": "~4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "npm run lint",
    "*.{js,ts,jsx,tsx,json,md,css,scss,yml}": "prettier --write **/*.{js,ts,jsx,tsx,json,md,css,scss,yml}"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
